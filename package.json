{
  "name": "openclaw-pine-voice",
  "version": "0.1.9",
  "description": "Make phone calls via Pine AI voice agent from OpenClaw",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ]
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "skills",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "openclaw",
    "plugin",
    "voice",
    "phone",
    "pine"
  ],
  "author": "Pine AI",
  "license": "MIT",
  "homepage": "https://pineclaw.com",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.32.0",
    "pine-voice": "^0.1.3"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
